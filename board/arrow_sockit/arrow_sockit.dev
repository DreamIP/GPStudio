<?xml version="1.0" encoding="UTF-8" ?>
<device name="arrow_sockit" description="SoCkit Cyclone 3 with USB/Eth communication and MT9/E2V image sensor">
	<svg width="200" height="100">
		<rect width="150" height="100" x="0" y="0" fill="green"/>
		<text x="35" y="53">Arrow Sockit</text>
		<path d="M150,40 L200,10 L200,90 L150,60" fill="black"/>
	</svg>
	<toochain name="altera_quartus_cyclone5" type="EP5CSXFC6D6F31"/>
	<global>
		<pins>
			<!--<pin name="PIN_J28" mapto="clk_50M"/>-->
		</pins>
	</global>
	<ios>
		<io name="led" type="him_output" driver="leds" optional="true" description="4 blue leds">
			<pins>
				<pin name="PIN_AF10" mapto="led0"/>
				<pin name="PIN_AD10" mapto="led1"/>
				<pin name="PIN_AE11" mapto="led2"/>
				<pin name="PIN_AD7" mapto="led3"/>
			</pins>
		</io>
		<io name="buttons" type="him_input" driver="buttons" optional="true" description="4 push buttons and 4 switch buttons">
			<pins>
				<pin name="PIN_AE9" mapto="key0"/>
				<pin name="PIN_AE12" mapto="key1"/>
				<pin name="PIN_AD9" mapto="key2"/>
				<pin name="PIN_AD11" mapto="key3"/>
				<pin name="PIN_W25" mapto="SW0"/>
				<pin name="PIN_V25" mapto="SW1"/>
				<pin name="PIN_AC28" mapto="SW2"/>
				<pin name="PIN_AC29" mapto="SW3"/>
			</pins>
		</io>
		<io name="mt9" type="image-sensor" driver="mt9" optional="false" optionalgroup="1" description="MT9 image sensor">
			<pins>
				<pin name="PIN_C3" mapto="mt9_data_i[0]"/>
				<pin name="PIN_F10" mapto="mt9_data_i[1]"/>
				<pin name="PIN_J10" mapto="mt9_data_i[2]"/>
				<pin name="PIN_B10" mapto="mt9_data_i[3]"/>
				<pin name="PIN_G11" mapto="mt9_data_i[4]"/>
				<pin name="PIN_G13" mapto="mt9_data_i[5]"/>
				<pin name="PIN_C9" mapto="mt9_data_i[6]"/>
				<pin name="PIN_F11" mapto="mt9_data_i[7]"/>
				<pin name="PIN_B12" mapto="mt9_data_i[8]"/>
				<pin name="PIN_E12" mapto="mt9_data_i[9]"/>
				<pin name="PIN_C11" mapto="mt9_data_i[10]"/>
				<pin name="PIN_A14" mapto="mt9_data_i[11]"/>
				<pin name="PIN_D5" mapto="mt9_extclk_o"/>
				<pin name="PIN_A10" mapto="mt9_fval_i"/>
				<pin name="PIN_H10" mapto="mt9_lval_i"/>
				<pin name="PIN_D12" mapto="mt9_oe_n_o"/>
				<pin name="PIN_B14" mapto="mt9_pixclk_i"/>
				<pin name="PIN_J13" mapto="mt9_reset_n_o"/>
				<pin name="PIN_F12" mapto="mt9_saddr_o"/>
				<pin name="PIN_C5" mapto="mt9_sclk_o"/>
				<pin name="PIN_D11" mapto="mt9_sdata_io"/>
				<pin name="PIN_A11" mapto="mt9_standby_o"/>
				<pin name="PIN_H12" mapto="mt9_trigger_o"/>
			</pins>
		</io>
		<io name="e2v" type="image-sensor" driver="e2v" optional="false" optionalgroup="1" description="E2V image sensor">
			<pins>
				<pin name="PIN_D5" mapto="clk_ref_o"/>
				<pin name="PIN_H10" mapto="csn_o[0]"/>
				<pin name="PIN_B14" mapto="data_clk_i"/>
				<pin name="PIN_E5" mapto="data_i[0]"/>
				<pin name="PIN_A3" mapto="data_i[1]"/>
				<pin name="PIN_A4" mapto="data_i[2]"/>
				<pin name="PIN_A6" mapto="data_i[3]"/>
				<pin name="PIN_C4" mapto="data_i[4]"/>
				<pin name="PIN_G7" mapto="data_i[5]"/>
				<pin name="PIN_E7" mapto="data_i[6]"/>
				<pin name="PIN_H8" mapto="data_i[7]"/>
				<pin name="PIN_E8" mapto="data_i[8]"/>
				<pin name="PIN_D7" mapto="data_i[9]"/>
				<pin name="PIN_B8" mapto="fen_i"/>
				<pin name="PIN_A10" mapto="flo_i"/>
				<pin name="PIN_A7" mapto="len_i"/>
				<pin name="PIN_G10" mapto="miso_i"/>
				<pin name="PIN_C8" mapto="mosi_o"/>
				<pin name="PIN_H12" mapto="reset_n_o"/>
				<pin name="PIN_C5" mapto="sck_o"/>
				<pin name="PIN_H13" mapto="trig_o"/>
				<pin name="PIN_A11" mapto="clk_fix_o"/>
				<pin name="PIN_D14" mapto="cs_var_no"/>
				<pin name="PIN_C13" mapto="sck_var_o"/>
				<pin name="PIN_F14" mapto="si_var_o"/>
			</pins>
		</io>
		<io name="usb" type="communication" driver="usb_cypress_CY7C68014A" optional="false" optionalgroup="2" description="">
			<pins>
				<pin name="PIN_AD11" mapto="usb_addr[0]"/>
				<pin name="PIN_AH12" mapto="usb_addr[1]"/>
				<pin name="PIN_AE7" mapto="usb_fd_io[0]"/>
				<pin name="PIN_AC7" mapto="usb_fd_io[1]"/>
				<pin name="PIN_AG7" mapto="usb_fd_io[2]"/>
				<pin name="PIN_AG6" mapto="usb_fd_io[3]"/>
				<pin name="PIN_AF9" mapto="usb_fd_io[4]"/>
				<pin name="PIN_AB9" mapto="usb_fd_io[5]"/>
				<pin name="PIN_AG10" mapto="usb_fd_io[6]"/>
				<pin name="PIN_AF10" mapto="usb_fd_io[7]"/>
				<pin name="PIN_AF13" mapto="usb_fd_io[8]"/>
				<pin name="PIN_Y15" mapto="usb_fd_io[9]"/>
				<pin name="PIN_AB14" mapto="usb_fd_io[10]"/>
				<pin name="PIN_Y14" mapto="usb_fd_io[11]"/>
				<pin name="PIN_AE14" mapto="usb_fd_io[12]"/>
				<pin name="PIN_AC14" mapto="usb_fd_io[13]"/>
				<pin name="PIN_AD14" mapto="usb_fd_io[14]"/>
				<pin name="PIN_AF14" mapto="usb_fd_io[15]"/>
				<pin name="PIN_AD10" mapto="usb_flaga"/>
				<pin name="PIN_AA10" mapto="usb_flagb"/>
				<pin name="PIN_AH11" mapto="usb_flagc"/>
				<pin name="PIN_AD12" mapto="usb_flagd"/>
				<pin name="PIN_AH14" mapto="usb_ifclk"/>
				<pin name="PIN_AF11" mapto="usb_sloe"/>
				<pin name="PIN_AD8" mapto="usb_slrd"/>
				<pin name="PIN_AE8" mapto="usb_slwr"/>
				<pin name="PIN_AF12" mapto="usb_pktend"/>
			</pins>
		</io>
		<io name="ethernet" type="communication" driver="eth_marvell_88e1111_gmii" optional="false" optionalgroup="2" description="Marvell 88e1111 ethernet controler in GMII mode">
			<pins>
				<pin name="PIN_Y14" mapto="eth_reset_n_o"/>
				<pin name="PIN_AB14" mapto="eth_mdc_o"/>
				<pin name="PIN_Y15" mapto="eth_mdio_io"/>
				<pin name="gmii_i.tx_clk" map="PIN_AE5"/>
				<pin name="gmii_i.rx_clk" map="PIN_AE5"/>
				<pin name="gmii_i.rx_dv" map="PIN_AF9"/>
				<pin name="gmii_i.rx_er" map="PIN_AG10"/>
				<pin name="gmii_i.rx_data[0]" map="PIN_AB9"/>
				<pin name="gmii_i.rx_data[1]" map="PIN_AD8"/>
				<pin name="gmii_i.rx_data[2]" map="PIN_AB8"/>
				<pin name="gmii_i.rx_data[3]" map="PIN_AE8"/>
				<pin name="gmii_i.rx_data[4]" map="PIN_AG8"/>
				<pin name="gmii_i.rx_data[5]" map="PIN_AE7"/>
				<pin name="gmii_i.rx_data[6]" map="PIN_AC7"/>
				<pin name="gmii_i.rx_data[7]" map="PIN_AG7"/>
				<pin name="gmii_i.crs_clk" map="PIN_AH3"/>
				<pin name="gmii_i.col_clk" map="PIN_AE4"/>
				<pin name="gmii_o.gtx_clk" map="PIN_AF5"/>
				<pin name="gmii_o.tx_en" map="PIN_AD10"/>
				<pin name="gmii_o.tx_er" map="PIN_AF10"/>
				<pin name="gmii_o.tx_data[0]" map="PIN_AA10"/>
				<pin name="gmii_o.tx_data[1]" map="PIN_AH11"/>
				<pin name="gmii_o.tx_data[2]" map="PIN_AF11"/>
				<pin name="gmii_o.tx_data[3]" map="PIN_AD11"/>
				<pin name="gmii_o.tx_data[4]" map="PIN_AH12"/>
				<pin name="gmii_o.tx_data[5]" map="PIN_AF12"/>
				<pin name="gmii_o.tx_data[6]" map="PIN_AD12"/>
				<pin name="gmii_o.tx_data[7]" map="PIN_AF13"/>
			</pins>
		</io>
		<io name="ethernet" type="communication" driver="eth_marvell_88e1111_rgmii" optional="false" optionalgroup="2" description="Marvell 88e1111 ethernet controler in RGMII mode">
			<pins>
				<pin name="PIN_Y14" mapto="eth_reset_n_o"/>
				<pin name="PIN_AB14" mapto="eth_mdc_o"/>
				<pin name="PIN_Y15" mapto="eth_mdio_io"/>
				<pin name="rgmii_i.rx_clk" map="PIN_AE5"/>
				<pin name="rgmii_i.rx_ctrl" map="PIN_AF9"/>
				<pin name="rgmii_i.rx_data[0]" map="PIN_AB9"/>
				<pin name="rgmii_i.rx_data[1]" map="PIN_AD8"/>
				<pin name="rgmii_i.rx_data[2]" map="PIN_AB8"/>
				<pin name="rgmii_i.rx_data[3]" map="PIN_AE8"/>
				<pin name="rgmii_o.gtx_clk" map="PIN_AF5"/>
				<pin name="rgmii_o.tx_ctrl" map="PIN_AD10"/>
				<pin name="rgmii_o.tx_data[0]" map="PIN_AA10"/>
				<pin name="rgmii_o.tx_data[1]" map="PIN_AH11"/>
				<pin name="rgmii_o.tx_data[2]" map="PIN_AF11"/>
				<pin name="rgmii_o.tx_data[3]" map="PIN_AD11"/>
			</pins>
		</io>
	</ios>
</device>
