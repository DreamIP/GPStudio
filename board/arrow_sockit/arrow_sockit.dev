<?xml version="1.0" encoding="UTF-8" ?>
<device name="arrow_sockit" description="SoCkit Cyclone 3 with USB/Eth communication and MT9/E2V image sensor">
	<svg width="200" height="100">
		<rect width="150" height="100" x="0" y="0" fill="green"/>
		<text x="35" y="53">Arrow Sockit</text>
		<path d="M150,40 L200,10 L200,90 L150,60" fill="black"/>
	</svg>
	<toochain name="altera_quartus_cyclone5" type="EP5CSXFC6D6F31"/>
	<global>
		<pins>
			<!--<pin name="clk_50M" map="PIN_J28"/>-->
		</pins>
	</global>
	<ios>
		<io name="led" type="him_output" driver="leds" optional="true" description="4 blue leds">
			<pins>
				<pin name="led0" map="PIN_AF10"/>
				<pin name="led1" map="PIN_AD10"/>
				<pin name="led2" map="PIN_AE11"/>
				<pin name="led3" map="PIN_AD7"/>
			</pins>
		</io>
		<io name="buttons" type="him_input" driver="buttons" optional="true" description="4 push buttons and 4 switch buttons">
			<pins>
				<pin name="key0" map="PIN_AE9"/>
				<pin name="key1" map="PIN_AE12"/>
				<pin name="key2" map="PIN_AD9"/>
				<pin name="key3" map="PIN_AD11"/>
				<pin name="SW0" map="PIN_W25"/>
				<pin name="SW1" map="PIN_V25"/>
				<pin name="SW2" map="PIN_AC28"/>
				<pin name="SW3" map="PIN_AC29"/>
			</pins>
		</io>
		<io name="mt9" type="image-sensor" driver="mt9" optional="false" optionalgroup="1" description="MT9 image sensor">
