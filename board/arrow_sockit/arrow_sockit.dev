				<pin name="mt9_trigger_o" map="PIN_H12"/>
			</pins>
		</io>
		<io name="e2v" type="image-sensor" driver="e2v" optional="false" optionalgroup="1" description="E2V image sensor">
			<pins>
				<pin name="clk_ref_o" map="PIN_D5"/>
				<pin name="csn_o[0]" map="PIN_H10"/>
				<pin name="data_clk_i" map="PIN_B14"/>
				<pin name="data_i[0]" map="PIN_E5"/>
