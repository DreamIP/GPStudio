				<pin name="mt9_data_i[6]" map="PIN_C9"/>
				<pin name="mt9_data_i[7]" map="PIN_F11"/>
				<pin name="mt9_data_i[8]" map="PIN_B12"/>
				<pin name="mt9_data_i[9]" map="PIN_E12"/>
				<pin name="mt9_data_i[10]" map="PIN_C11"/>
				<pin name="mt9_data_i[11]" map="PIN_A14"/>
